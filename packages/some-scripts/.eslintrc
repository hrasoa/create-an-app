{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {  
    "object-shorthand": "off",
    "func-names": "off",
    "import/no-dynamic-require": "off",
    "react/jsx-filename-extension": ["warn", { "extensions": [".js"] }]
  }
}